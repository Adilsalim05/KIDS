<?xml version="1.0"?>
<rules>
	<rule label="1">
		<feature if="confers resistance to" then="confers resistance to antibiotic"></feature>
		<feature if="is upregulated by" then="is upregulated by antibiotic"></feature>
		<feature if="upregulated by antibiotic after 30 mins" then="upregulated by antibiotic after 30 mins"></feature>
		<feature if="is mutated by" then="is mutated by"></feature>
		<feature if="is associated with resistance to" then="is associated with resistance to"></feature>
		<feature if="has" then="has"></feature>
		<feature if="is involved in" then="is involved in"></feature>
		<feature if="is part of" then="is part of"></feature>
		<feature if="targeted by" then="targeted by"></feature>
	</rule>
	<rule label="-1">
		<feature if="confers no resistance to drug" then="confers resistance to antibiotic"></feature>
		<feature if="is not regulated by" then="is regulated by antibiotic"></feature>
		<feature if="is not mutated by" then="represses"></feature>
	</rule>
</rules>
